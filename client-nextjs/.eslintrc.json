{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "react/display-name": "off",
    "react/jsx-key": "off",
    "@next/next/no-html-link-for-pages": "off",
    "react/no-unescaped-entities": 0,
    "no-console": "warn",
    "no-unused-vars": "error"
  }
}