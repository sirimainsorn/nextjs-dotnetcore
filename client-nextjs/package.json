    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.7",
    "postcss": "^8.4.12",
    "prettier": "2.6.1",
    "tailwindcss": "^3.0.23"
