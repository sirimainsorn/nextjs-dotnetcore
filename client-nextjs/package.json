  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
